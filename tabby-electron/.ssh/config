Include config.d/*

# Absolute PATH
Include /etc/ssh/test_ssh_config

# Recurse
Include config.recurse.d/**/*

# Not include same file twice
Include config.d/config

# Handle not Found
Include not-found.conf

# No overwrite
Host twice.example.com
  HostName 1.2.3.4

# Handle Include in block
Host in-block.example.com
  Include in-block.conf
